<view class="page"><view class="tabs"><view class="tabs_title"><block tt:for="{{tabs}}" tt:for-item="item" tt:for-index="index"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{(('titleItem')+' '+((tabNum==index)?'active':''))}}" bindtap="__e">{{''+item.title+''}}</view></block></view><view class="{{(('tabContent')+' '+((tabNum==0)?'dis':''))}}">0</view><view class="{{(('tabContent')+' '+((tabNum==1)?'dis':''))}}">1</view><view class="{{(('tabContent')+' '+((tabNum==2)?'dis':''))}}">2</view></view></view>